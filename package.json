    "eslint": "2.11.1",
    "eslint-config-360fy": "1.1.9",
    "events": "1.1.0",
    "immutable": "3.8.1",
    "lodash": "4.13.1",
    "moment": "2.13.0",
    "qs": "6.2.0",
    "react": "15.1.0",
    "react-dom": "15.1.0",
    "react-router": "2.4.1",
    "reactjs-web-boilerplate": "1.2.4",
    "socket.io-client": "1.4.6"
  },
  "devDependencies": {
    "babel-core": "6.9.1",
    "babel-plugin-transform-runtime": "6.9.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.5.0",
    "eslint": "2.11.1",
    "eslint-config-360fy": "1.1.9",
    "eslint-plugin-react": "5.1.1"
  },
  "eslintConfig": {
    "extends": "360fy"
  },
  "babel": {
    "plugins": [
      "transform-runtime"

    "events": "1.1.1",
    "immutable": "3.8.1",
    "lodash": "4.17.2",
    "moment": "2.16.0",
    "qs": "6.3.0",
    "react": "15.4.0",
    "react-dom": "15.4.0",
    "react-router": "3.0.0",
    "reactjs-web-boilerplate": "1.2.6",
    "socket.io-client": "1.5.1"
  },
  "devDependencies": {
    "babel-core": "6.18.2",
    "babel-plugin-transform-runtime": "6.15.0",
    "babel-preset-node5": "11.1.0",
    "babel-preset-react": "6.16.0",
    "eslint": "3.10.2",
    "eslint-config-360fy": "1.1.12",
    "eslint-plugin-react": "6.7.1"
  },
  "eslintConfig": {
    "extends": "360fy"
